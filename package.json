{
  "name": "chatgpt-to-remotion-interface",
  "version": "1.0.0",
  "description": "Convert JSON presentations to Remotion videos",
  "main": "src/index.ts",
  "scripts": {
    "start": "remotion preview",
    "build": "remotion render Presentation output/video.mp4",
    "render": "node dist/cli.ts",
    "build:cli": "tsc",
    "dev": "tsx src/cli.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"*.{json,md}\"",
    "inspect": "tsx --inspect-brk src/cli.ts"
  },
  "keywords": [
    "remotion",
    "presentation",
    "video",
    "ppt"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/text-to-speech": "^6.4.0",
    "@remotion/bundler": "^4.0.136",
    "@remotion/cli": "^4.0.136",
    "@remotion/renderer": "^4.0.136",
    "commander": "^11.1.0",
    "lodash": "^4.17.23",
    "music-metadata": "^11.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remotion": "^4.0.136",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.23",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
